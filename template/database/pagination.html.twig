<nav class="nav-page" aria-label="pagination">
<div>
<a class="nav-page-first"{% if pagination.hasPreviousPage() %} href="?page=1"{% endif %}>First</a>
<a class="nav-page-previous"{% if pagination.hasPreviousPage() %} href="?page={{ pagination.getPreviousPage() }}"{% endif %}>Previous</a>
<a class="nav-page-next"{% if pagination.hasNextPage() %} href="?page={{ pagination.getNextPage() }}"{% endif %}>Next</a>
<a class="nav-page-last"{% if pagination.hasNextPage() %} href="?page={{ pagination.getNbPages() }}"{% endif %}>Last</a>
</div>
<div>
<a aria-current="page">
    {{ pagination_rows|default('Rows') }}
    {{ pagination.getCurrentPageOffsetStart()|number_format }} â€“
    {{ pagination.getCurrentPageOffsetEnd()|number_format }} of
    {{ pagination.getNbResults()|number_format }}
    {{ pagination_sort }}
</a>
{% if pagination_file %}
<a href="{{ current_url() }}/{{ pagination_file }}">Download as {{ pagination_file|upper }}</a>
{% endif %}
</div>
</nav>
